{
    "name": "elefan-grenoble/gestion-compte",
    "license": "GPLv3",
    "type": "project",
    "description": "Web site to manage the cooperative grocery shop l'Elefan",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.14",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^1.0",
        "egulias/email-validator": "^2.1",
        "endroid/qr-code": "^4.6",
        "friendsofsymfony/oauth-server-bundle": "^1.5",
        "friendsofsymfony/user-bundle": "^2.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "knpuniversity/oauth2-client-bundle": "1.34",
        "liip/imagine-bundle": "^2.0",
        "michelf/php-markdown": "^1.7",
        "nesbot/carbon": "^2.36",
        "ornicar/gravatar-bundle": "^1.1",
        "picqer/php-barcode-generator": "^2.4",
        "sensio/framework-extra-bundle": "^6.0",
        "spipu/html2pdf": "^5.2",
        "stevenmaguire/oauth2-keycloak": "^4.0",
        "symfony/asset": "4.4.*",
        "symfony/browser-kit": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/security": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9",
        "symfony/phpunit-bridge": "^5.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
