# Shifting Configuration
DUE_DURATION_BY_CYCLE=180
MIN_SHIFT_DURATION=90
CYCLE_DURATION='28 days'
CYCLE_TYPE=abcd
NEW_USERS_START_AS_BEGINNER=true
ALLOW_EXTRA_SHIFTS=true
